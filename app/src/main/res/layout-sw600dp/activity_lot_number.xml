        <AutoCompleteTextView
            android:id="@+id/lot_partname"
            android:layout_width="281dp"
            android:layout_height="62dp"
            android:layout_marginStart="16dp"
            android:background="@drawable/border"
            android:fontFamily="@font/arbutus_slab"
